datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model CitizenTokens {
  id        Int      @id @default(autoincrement())
  citizenId Int
  token     String
  used      Boolean  @default(false)
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt

  @@map("CitizenTokens")
}
